<!DOCTYPE html>
<html>
  <head>
    <title>Bloginsta</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
  
  
  <div class="jumbotron text-center" style="margin-bottom:0">
  <h1>My Instagram Page</h1>
   <%= image_tag("logos/blackiinsta.jpg", width: "100px") %>
   </div>
  
    
    <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "#{key}") %>
    <% end %>
    
    <% if logged_in? %>
      <%= link_to "Profile", user_path(current_user.id) %>
      <%= link_to "Logout", session_path(current_user.id), method: :delete %>
    </div>
    <div id="gnavi" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li><%= link_to 'Feeds',feeds_path %></li>
        <li><%= link_to 'Instagram',blogs_path %></li>
      </ul>
    </div>
  </div>
      
    <% else %>
      <%= link_to "Sign up", new_user_path %>
      <%= link_to "Login", new_session_path %>
    <% end %>
    
    </nav>
    
    <%= yield %>
    
    
    <footer>
       <p>Operating company DIVE INTO CODE</p>
       <p>Â© 2015-2016 Copyright DIVE INTO CODE Corp.</p>
  </footer>
  </body>
</html>
